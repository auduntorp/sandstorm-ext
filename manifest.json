{
    "manifest_version": 2,
    "name": "Sandstorm",
    "version": "0.0.1",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["libs/tfjs.min.js", "libs/coco-ssd.min.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}